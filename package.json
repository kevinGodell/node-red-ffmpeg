{
  "name": "@kevingodell/node-red-ffmpeg",
  "version": "0.1.1-beta.1",
  "description": "Spawn a long-running ffmpeg process to handle video/image processing.",
  "main": "ffmpeg.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "npm pack",
    "pretty": "prettier --write **/*.html",
    "lint": "eslint --fix --ext .js --ignore-path .gitignore .",
    "clean": "npm run lint && npm run pretty",
    "beta": "npm version prerelease --preid=beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinGodell/node-red-ffmpeg.git"
  },
  "keywords": [
    "node-red",
    "ffmpeg"
  ],
  "author": "Kevin Godell <kevin.godell@gmail.com> (https://github.com/kevinGodell)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevinGodell/node-red-ffmpeg/issues"
  },
  "homepage": "https://github.com/kevinGodell/node-red-ffmpeg#readme",
  "devDependencies": {
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1"
  },
  "files": [
    "ffmpeg.html",
    "locales",
    "examples"
  ],
  "node-red": {
    "version": ">=3.0.2",
    "nodes": {
      "ffmpeg": "ffmpeg.js"
    }
  },
  "engines": {
    "node": ">=14"
